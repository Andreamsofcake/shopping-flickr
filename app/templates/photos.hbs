<div class="content">
  <h1>Shoppy Shoppy Flickr</h1>
  {{#link-to 'cart' class= "cart"}}Cart{{/link-to}}
  <ul>
    {{#each}}
      <li>{{title}} <button {{action 'addPhotoToCart' this}}> Add to Cart </button>
      <img {{bind-attr src= 'photoURL'}}></img>
      </li>
    {{/each}}
  </ul>
</div>

